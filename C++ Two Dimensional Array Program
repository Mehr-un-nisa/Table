/* C++ Program - Two Dimensional Array Program */
		
#include<iostream.h>
#include<conio.h>
void main()
{
	clrscr();
	int arr[10][10], row, col, i, j;
	cout<<"Enter number of row for Array (max 10) : ";
	cin>>row;
	cout<<"Enter number of column for Array (max 10) : ";
	cin>>col;
	cout<<"Now Enter "<<row<<"*"<<col<<" Array Elements : ";
	for(i=0; i<row; i++)
	{
		for(j=0; j<col; j++)
		{
			cin>>arr[i][j];
		}
	}
	cout<<"The Array is :\n";
	for(i=0; i<row; i++)
	{
		for(j=0; j<col; j++)
		{
			cout<<arr[i][j]<<" ";
		}
		cout<<"\n";
	}
	getch();
}
